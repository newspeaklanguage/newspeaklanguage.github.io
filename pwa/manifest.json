{
  "name": "newspeak-ide-pwa",
  "version": "0.0.1",
  "description": "The Newspeak WEB IDE as PWA.",
  "main": "index.js",
  "scripts": {
    "dev-server": "vite --open",
    "dev": "npx --y @azure/static-web-apps-cli start http://localhost:3000 --run 'npm run dev-server'",
    "dev-task": "vite",
    "build": "tsc && vite build",
    "serve": "npm run build && npx --y http-server ./dist",
    "start": "npm run dev",
    "start-remote": "vite --host"
  },
  "author": "Mircea Trofimciuc",
  "license": "ISC",
  "dependencies": {
    "@pwabuilder/pwainstall": "^1.6.7",
    "@vaadin/router": "^1.7.4",
    "lit": "^2.2.1",
    "workbox-build": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-precaching": "^6.5.2"
  },
  "devDependencies": {
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vite-plugin-pwa": "^0.11.13"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  }
}
